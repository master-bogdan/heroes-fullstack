{
  "name": "fullstack",
  "version": "0.1.0",
  "private": true,
  "main": "build/app.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17"
  },
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node build/app.js",
