{
  "name": "fullstack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc --skipLibCheck",
    "server": "nodemon build/index.js",
    "tsc": "tsc -w",
    "postinstall": "tsc --skipLibCheck",
    "dev": "concurrently -n 'tsc,server,client' -c 'red,yellow,blue' \"npm run tsc\" \"npm run server\" \"cd ../ && npm run start --prefix client\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.4"
  }
}
